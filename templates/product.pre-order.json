{
  "wrapper": "div.product-template-wrapper",
  "sections": {
    "rich-text": {
      "type": "ship-rich-text",
      "blocks": {
        "subheading": {
          "type": "subheading",
          "settings": {
          }
        },
        "heading": {
          "type": "heading",
          "settings": {
          }
        }
      },
      "block_order": [
        "subheading",
        "heading"
      ],
      "settings": {
        "section_width": "full-with-padding",
        "horizontal_alignment": "text-center justify-center",
        "section_color_scheme": "accent-3"
      }
    },
    "product-main-gallery": {
      "type": "flotilla-product-main-gallery",
      "settings": {
        "gallery_style": "thumbnails",
        "enable_zoom": true,
        "enable_video_looping": true
      }
    },
    "product-main-form": {
      "type": "flotilla-product-main-form",
      "blocks": {
        "title": {
          "type": "title",
          "settings": {
            "show_vendor": false,
            "show_sku": false
          }
        },
        "price": {
          "type": "price",
          "settings": {
            "show_tax_included_text": false,
            "show_shipping_policy": false
          }
        },
        "stock-level-indicator": {
          "type": "stock-level-indicator",
          "settings": {
            "stock_range": 5,
            "show_stock_count": false,
            "in_stock_message": "Ready to ship",
            "low_stock_message": "Low in stock!",
            "out_of_stock_message": "Back in soon",
            "preorder_message": "Coming soon! In stock 01 Jan 2023"
          }
        },
        "options": {
          "type": "options",
          "settings": {
            "options_type": "dropdowns",
            "preselect_variant": true,
            "color_swatch_identifier": "color,colour,couleur,colore,farbe,색,色,färg,farve"
          }
        },
        "highlight": {
          "type": "highlight",
          "settings": {
          }
        },
        "payment-buttons": {
          "type": "payment-buttons",
          "settings": {
            "preorder": true
          }
        },
        "description": {
          "type": "description",
          "settings": {
            "enable_accordion": true,
            "description_heading": "Details"
          }
        }
      },
      "block_order": [
        "title",
        "price",
        "stock-level-indicator",
        "options",
        "highlight",
        "payment-buttons",
        "description"
      ],
      "settings": {
      }
    },
    "specifications": {
      "type": "flotilla-product-specifications",
      "settings": {
        "subheading": "",
        "heading": "",
