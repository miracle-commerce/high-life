{
  "wrapper": "div.product-template-wrapper",
  "sections": {
    "product-main-gallery": {
      "type": "flotilla-product-main-gallery",
      "settings": {
        "gallery_style": "slider",
        "enable_zoom": true,
        "enable_video_looping": true
      }
    },
    "product-main-form": {
      "type": "flotilla-product-main-form",
      "blocks": {
        "title": {
          "type": "title",
          "settings": {
            "show_vendor": false,
            "show_sku": false
          }
        },
        "price": {
          "type": "price",
          "settings": {
            "show_tax_included_text": false,
            "show_shipping_policy": false
          }
        },
        "ask-a-question": {
          "type": "ask-a-question",
          "settings": {
            "title": "Ask a question",
            "test_mode": false,
            "form_heading": "Got a question?",
            "form_subheading": "Enter your question below and we will get back to you as soon as possible.",
            "name_input": "required",
            "phone_input": "required"
          }
        },
        "options": {
          "type": "options",
          "settings": {
            "options_type": "dropdowns",
            "preselect_variant": true,
            "color_swatch_identifier": "color,colour,couleur,colore,farbe,색,色,färg,farve"
          }
        },
        "quantity": {
          "type": "quantity",
          "settings": {
          }
        },
        "highlight": {
          "type": "highlight",
          "settings": {
          }
        },
        "payment-buttons": {
          "type": "payment-buttons",
          "settings": {
            "preorder": false
          }
        },
        "local-pickup-availability": {
          "type": "local-pickup-availability",
          "settings": {
            "pickup_color_scheme": "general"
          }
        },
        "description": {
          "type": "description",
          "settings": {
            "enable_accordion": true,
            "description_heading": "Details"
          }
        }
      },
      "block_order": [
        "title",
        "price",
        "ask-a-question",
        "options",
        "quantity",
        "highlight",
        "payment-buttons",
        "local-pickup-availability",
        "description"
      ],
      "settings": {
      }
    },
    "specifications": {
