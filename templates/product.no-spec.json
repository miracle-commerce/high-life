      },
      "block_order": [
        "subheading",
        "heading",
        "text",
        "buttons"
      ],
      "settings": {
        "section_width": "narrow",
        "horizontal_alignment": "text-left justify-start",
        "section_color_scheme": "accent-1"
      }
    },
    "accordion": {
      "type": "ship-accordion-section",
      "blocks": {
        "accordion-00": {
          "type": "tab_item",
          "settings": {
          }
        },
        "accordion-01": {
          "type": "tab_item",
          "settings": {
          }
        },
        "accordion-02": {
          "type": "tab_item",
          "settings": {
