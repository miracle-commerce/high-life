{
  "wrapper": "div.product-template-wrapper",
  "sections": {
    "gallery": {
      "type": "flotilla-product-main-gallery",
      "settings": {
        "gallery_style": "thumbnails",
        "enable_zoom": true,
        "enable_video_looping": true
      }
    },
    "main": {
      "type": "flotilla-product-main-form",
      "blocks": {
        "title": {
          "type": "title",
          "settings": {
            "show_vendor": false,
            "show_sku": false
          }
        },
        "price": {
          "type": "price",
          "settings": {
            "show_tax_included_text": false,
            "show_shipping_policy": false
          }
        },
        "options": {
          "type": "options",
          "settings": {
            "options_type": "buttons",
            "preselect_variant": true,
            "color_swatch_identifier": "color,colour,couleur,colore,farbe,색,色,färg,farve"
          }
        },
        "quantity": {
          "type": "quantity",
          "settings": {
          }
        },
        "payment-buttons": {
          "type": "payment-buttons",
          "settings": {
            "preorder": false
          }
        },
        "local-pickup-availability": {
          "type": "local-pickup-availability",
          "settings": {
            "pickup_color_scheme": "general"
          }
        },
        "description": {
          "type": "description",
          "settings": {
            "enable_accordion": true,
            "description_heading": "Details"
          }
        }
      },
      "block_order": [
        "title",
        "price",
        "options",
        "quantity",
        "payment-buttons",
        "local-pickup-availability",
        "description"
      ],
      "settings": {
      }
    },
    "icons-with-text-00": {
      "type": "flotilla-icons-with-text",
      "blocks": {
        "icons-with-text-00": {
          "type": "icon_with_text",
          "settings": {
          }
        },
        "icons-with-text-01": {
          "type": "icon_with_text",
          "settings": {
          }
        },
        "icons-with-text-02": {
          "type": "icon_with_text",
          "settings": {
          }
        }
      },
      "block_order": [
        "icons-with-text-00",
        "icons-with-text-01",
        "icons-with-text-02"
      ],
      "settings": {
        "text_alignment": "left",
        "section_width": "full",
        "section_color_scheme": "accent-2"
      }
    },
    "testimonials": {
      "type": "ship-testimonials",
      "blocks": {
        "testimonials-00": {
          "type": "testimonial",
          "settings": {
          }
        }
      },
      "block_order": [
        "testimonials-00"
      ],
      "settings": {
        "section_color_scheme": "accent-1"
      }
    },
    "featured-collection": {
      "type": "flotilla-featured-collection",
      "blocks": {
        "image": {
          "type": "image",
          "settings": {
            "product_cards_aspect_ratio": "tall",
            "product_cards_crop_position": "object-center",
            "collection_hover_effect": "show_second_image"
          }
        },
        "title": {
          "type": "title",
          "settings": {
          }
        },
        "price": {
          "type": "price",
          "settings": {
          }
        }
      },
      "block_order": [
        "image",
        "title",
        "price"
      ],
      "settings": {
        "alignment": "text-left",
        "products_per_page": 5,
        "products_per_row": 5,
        "color_scheme": "general",
        "product_card_alignment": "left"
      }
    },
    "icons-with-text-01": {
      "type": "flotilla-icons-with-text",
      "blocks": {
        "icons-with-text-01-00": {
          "type": "icon_with_text",
          "settings": {
          }
        },
        "icons-with-text-01-01": {
          "type": "icon_with_text",
          "settings": {
          }
        },
        "icons-with-text-01-02": {
          "type": "icon_with_text",
          "settings": {
          }
        }
      },
      "block_order": [
        "icons-with-text-01-00",
        "icons-with-text-01-01",
        "icons-with-text-01-02"
      ],
      "settings": {
        "text_alignment": "center",
        "section_width": "full-with-padding",
        "section_color_scheme": "accent-3"
      }
    },
    "rich-text": {
      "type": "ship-rich-text",
      "blocks": {
        "subheading": {
          "type": "subheading",
          "settings": {
          }
        },
        "heading": {
          "type": "heading",
          "settings": {
          }
        },
        "text": {
          "type": "text",
          "settings": {
          }
        },
        "buttons": {
          "type": "buttons",
          "settings": {
            "button_1_link": "shopify:\/\/collections\/all",
