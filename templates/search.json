{
  "wrapper": "div.flex.w-full.justify-center.flex-wrap.product-template-wrapper.flex-col",
  "sections": {
    "main": {
      "type": "ship-search-navigation",
      "settings": {
      }
    },
    "products-filter-search-results": {
      "type": "flotilla-search-filters-topbar",
      "settings": {
        "enable_filtering": true,
        "enable_sorting": true
      }
    },
    "products-search-results": {
      "type": "flotilla-search-products",
      "blocks": {
        "product_card_image": {
          "type": "image",
          "settings": {
            "product_cards_aspect_ratio": "tall",
            "product_cards_crop_position": "object-center",
            "collection_hover_effect": "show_second_image"
          }
        },
        "product_card_title": {
          "type": "title",
          "settings": {
          }
        },
        "product_card_price": {
          "type": "price",
          "settings": {
          }
        },
        "view_details_quick_add": {
          "type": "view_details_quick_add",
          "settings": {
            "enable_view_details": true,
            "view_details_open": false,
            "view_details_open_all": true,
            "show_quick_add": true
          }
        }
      },
      "block_order": [
        "product_card_image",
        "product_card_title",
        "product_card_price",
        "view_details_quick_add"
      ],
      "settings": {
        "products_per_row": 4,
        "products_per_page": 12,
        "product_card_alignment": "left"
      }
    },
    "collections-search-results": {
      "type": "flotilla-search-collections",
      "settings": {
        "collections_per_row_desktop": 3,
        "collections_per_row_mobile": "1",
        "overlay_color_scheme": "bg-black",
        "overlay_opacity": 20,
        "text_overlay_color_scheme": "accent-1",
        "vertical_text_alignment": "bottom",
        "horizontal_text_alignment": "left",
        "button_label": "Shop now",
        "button_style": "filled"
      }
    },
    "article-search-results": {
      "type": "flotilla-search-blog",
      "settings": {
        "image_size": "square",
        "text_alignment": "left",
        "show_date": true,
        "show_author": false
      }
    },
    "page-search-results": {
